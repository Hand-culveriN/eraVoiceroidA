;-------------------------------------------------
;苦痛快楽経験、奉仕快楽経験のチェック
;-------------------------------------------------
@EXP_GOT_CHECK
;奉仕系である、奉仕快楽のチェック
A = UP:快Ｃ+UP:快Ｖ+UP:快Ａ+UP:快Ｂ
E = 0

IF UP:欲情 < 100
	A = 0
ELSEIF UP:欲情 < 300
	A *= 2
ELSEIF UP:欲情 < 700
	A *= 3
ELSEIF UP:欲情 < 1500
	A *= 4
ELSE
	A *= 5
ENDIF

IF TFLAG:100
	IF A >= 12000
		E = 16
		TIMES UP:恐怖 , 0.65
		TIMES UP:反感 , 0.30
	ELSEIF A >= 8000
		E = 12
		TIMES UP:恐怖 , 0.70
		TIMES UP:反感 , 0.40
	ELSEIF A >= 5000
		E = 8
		TIMES UP:恐怖 , 0.75
		TIMES UP:反感 , 0.50
	ELSEIF A >= 3000
		E = 4
		TIMES UP:恐怖 , 0.80
		TIMES UP:反感 , 0.60
	ELSEIF A >= 2000
		E = 2
		TIMES UP:恐怖 , 0.85
		TIMES UP:反感 , 0.70
	ELSEIF A >= 1000
		E = 1
		TIMES UP:恐怖 , 0.90
		TIMES UP:反感 , 0.80
	ENDIF
	IF E
		PRINT 奉仕快楽経験+
		PRINTVL E
		EXP:奉仕快楽経験 += E
		TFLAG:26 = E
	ENDIF
ENDIF

;苦痛快楽のチェック
E = 0
IF UP:欲情 >= 3000 && UP:苦痛 >= 2000
	E = 16
	TIMES UP:恐怖 , 0.65
ELSEIF UP:欲情 >= 2500 && UP:苦痛 >= 1500
	E = 12
	TIMES UP:恐怖 , 0.70
ELSEIF UP:欲情 >= 1500 && UP:苦痛 >= 1000
	E = 8
	TIMES UP:恐怖 , 0.75
ELSEIF UP:欲情 >= 1000 && UP:苦痛 >= 500
	E = 4
	TIMES UP:恐怖 , 0.80
ELSEIF UP:欲情 >= 600 && UP:苦痛 >= 300
	E = 2
	TIMES UP:恐怖 , 0.85
ELSEIF UP:欲情 >= 300 && UP:苦痛 >= 100
	E = 1
	TIMES UP:恐怖 , 0.90
ENDIF
IF E > 0
	PRINT 苦痛快楽経験+
	PRINTVL E
	EXP:苦痛快楽経験 += E
	TFLAG:27 = E
