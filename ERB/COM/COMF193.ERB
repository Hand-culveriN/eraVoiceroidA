;-------------------------------------------------
;コンドーム精飲(助手)
;特殊コマンド
;-------------------------------------------------
@COM193
PRINTL コンドーム精飲(助手)
STR:0 = コンドーム精飲(助手)
CALL KOJO_MESSAGE_COM


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 0
LOSEBASE:気力 += 30

SOURCE:性行動 = 100
SOURCE:液体追加 = 100
SOURCE:逸脱 = 800
SOURCE:快Ｂ = 200
SOURCE:18 = 200


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:恭順追加 += 50

;助手と奴隷の相性によって、全体のソースが変化
T = NO:TARGET
A = NO:ASSI
SOURCE:性行動 *= RELATION:A
SOURCE:液体追加 *= RELATION:A
SOURCE:逸脱 *= RELATION:A
SOURCE:恭順追加 *= RELATION:A
SOURCE:快Ｂ *= RELATION:A
SOURCE:18 *= RELATION:A
SOURCE:性行動 /= 100
SOURCE:液体追加 /= 100
SOURCE:逸脱 /= 100
SOURCE:恭順追加 /= 100
SOURCE:快Ｂ /= 100
SOURCE:18 /= 100

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;助手の口に精液汚れ
STAIN:ASSI:口 |= 4

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;コンドームの精液を初期化
CFLAG:107 = 0


RETURN 1

