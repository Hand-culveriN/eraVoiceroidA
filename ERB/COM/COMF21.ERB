;-------------------------------------------------
;何もしない
;コミュニケーション系コマンド、レベル1
;-------------------------------------------------
@COM21
PRINTL 何もしない
STR:0 = 何もしない
CALL KOJO_MESSAGE_COM

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 10
LOSEBASE:気力 += 10

SOURCE:4 = 10
SOURCE:13 = 10
SOURCE:17 = 10


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;苦痛を伴わないマゾっ気及びマゾの伴う処理は先にしておく
;マゾっ気を見る
IF ABL:マゾっ気 == 0
	TIMES SOURCE:13 , 0.80
	SOURCE:10 += 0
	SOURCE:11 += 0
ELSEIF ABL:マゾっ気 == 1
	TIMES SOURCE:13 , 1.00
	SOURCE:10 += 20
	SOURCE:11 += 30
ELSEIF ABL:マゾっ気 == 2
	TIMES SOURCE:13 , 1.20
	SOURCE:10 += 40
	SOURCE:11 += 70
ELSEIF ABL:マゾっ気 == 3
	TIMES SOURCE:13 , 1.40
	SOURCE:10 += 70
	SOURCE:11 += 120
ELSEIF ABL:マゾっ気 == 4
	TIMES SOURCE:13 , 1.50
	SOURCE:10 += 110
	SOURCE:11 += 180
ELSEIF ABL:マゾっ気 == 5
	TIMES SOURCE:13 , 1.70
	SOURCE:10 += 150
	SOURCE:11 += 250
ELSEIF ABL:マゾっ気 == 6
	TIMES SOURCE:13 , 1.90
	SOURCE:10 += 200
	SOURCE:11 += 300
ELSEIF ABL:マゾっ気 == 7
	TIMES SOURCE:13 , 2.10
	SOURCE:10 += 250
	SOURCE:11 += 350
ELSEIF ABL:マゾっ気 == 8
	TIMES SOURCE:13 , 2.30
	SOURCE:10 += 300
	SOURCE:11 += 400
ELSEIF ABL:マゾっ気 == 9
	TIMES SOURCE:13 , 2.50
	SOURCE:10 += 350
	SOURCE:11 += 450
ELSEIF ABL:マゾっ気 >= 10
	TIMES SOURCE:13 , 3.00
	SOURCE:10 += 500
	SOURCE:11 += 500
ENDIF

;マゾを見る
IF TALENT:マゾ == 1
	TIMES SOURCE:13 , 2.50
	TIMES SOURCE:10 , 1.20
	TIMES SOURCE:11 , 1.20
ENDIF


;依存度ベクトル
TFLAG:33 = 0

RETURN 1

