;-------------------------------------------------
;何もしない
;コミュニケーション系コマンド、レベル1
;-------------------------------------------------
@COM21
PRINTL 何もしない
STR:0 = 何もしない
CALL KOJO_MESSAGE_COM

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 10
LOSEBASE:気力 += 10

SOURCE:性行動 = 10
SOURCE:屈従 = 10
SOURCE:快Ｂ = 10


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;苦痛を伴わないマゾっ気及びマゾの伴う処理は先にしておく
;マゾっ気を見る
IF ABL:マゾっ気 == 0
	TIMES SOURCE:屈従 , 0.80
	SOURCE:液体追加 += 0
	SOURCE:欲情追加 += 0
ELSEIF ABL:マゾっ気 == 1
	TIMES SOURCE:屈従 , 1.00
	SOURCE:液体追加 += 20
	SOURCE:欲情追加 += 30
ELSEIF ABL:マゾっ気 == 2
	TIMES SOURCE:屈従 , 1.20
	SOURCE:液体追加 += 40
	SOURCE:欲情追加 += 70
ELSEIF ABL:マゾっ気 == 3
	TIMES SOURCE:屈従 , 1.40
	SOURCE:液体追加 += 70
	SOURCE:欲情追加 += 120
ELSEIF ABL:マゾっ気 == 4
	TIMES SOURCE:屈従 , 1.50
	SOURCE:液体追加 += 110
	SOURCE:欲情追加 += 180
ELSEIF ABL:マゾっ気 == 5
	TIMES SOURCE:屈従 , 1.70
	SOURCE:液体追加 += 150
	SOURCE:欲情追加 += 250
ELSEIF ABL:マゾっ気 == 6
	TIMES SOURCE:屈従 , 1.90
	SOURCE:液体追加 += 200
	SOURCE:欲情追加 += 300
ELSEIF ABL:マゾっ気 == 7
	TIMES SOURCE:屈従 , 2.10
	SOURCE:液体追加 += 250
	SOURCE:欲情追加 += 350
ELSEIF ABL:マゾっ気 == 8
	TIMES SOURCE:屈従 , 2.30
	SOURCE:液体追加 += 300
	SOURCE:欲情追加 += 400
ELSEIF ABL:マゾっ気 == 9
	TIMES SOURCE:屈従 , 2.50
	SOURCE:液体追加 += 350
	SOURCE:欲情追加 += 450
ELSEIF ABL:マゾっ気 >= 10
	TIMES SOURCE:屈従 , 3.00
	SOURCE:液体追加 += 500
	SOURCE:欲情追加 += 500
ENDIF

;マゾを見る
IF TALENT:マゾ == 1
	TIMES SOURCE:屈従 , 2.50
	TIMES SOURCE:液体追加 , 1.20
	TIMES SOURCE:欲情追加 , 1.20
ENDIF


;依存度ベクトル
TFLAG:33 = 0

RETURN 1

