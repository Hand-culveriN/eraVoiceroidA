;-------------------------------------------------
;乳ファック
;セックス系コマンド、レベル1
;-------------------------------------------------
@COM65
PRINTL 乳ファック
;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
;共通処理
CALL COM_ORDER_COMMON
SIF RESULT == 0
	RETURN 0


;-------------------------------------------------
;実行決定
;-------------------------------------------------
STR:0 = 乳ファック
CALL KOJO_MESSAGE_COM

;-------------------------------------------------
;射精ゲージチェック
;-------------------------------------------------
B = 1500

;ABL:調教者の技巧をみる
IF ABL:PLAYER:技巧 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:技巧 == 1
	TIMES B , 1.20
ELSEIF ABL:PLAYER:技巧 == 2
	TIMES B , 1.40
ELSEIF ABL:PLAYER:技巧 == 3
	TIMES B , 1.60
ELSEIF ABL:PLAYER:技巧 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.00
ENDIF

;ABL:従順をみる
IF ABL:従順 == 0
	TIMES B , 0.80
ELSEIF ABL:従順 == 1
	TIMES B , 0.90
ELSEIF ABL:従順 == 2
	TIMES B , 1.00
ELSEIF ABL:従順 == 3
	TIMES B , 1.10
ELSEIF ABL:従順 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:欲望をみる
IF ABL:欲望 == 0
	TIMES B , 1.00
ELSEIF ABL:欲望 == 1
	TIMES B , 1.10
ELSEIF ABL:欲望 == 2
	TIMES B , 1.20
ELSEIF ABL:欲望 == 3
	TIMES B , 1.30
ELSEIF ABL:欲望 == 4
	TIMES B , 1.40
ELSE
	TIMES B , 1.50
ENDIF

;PALAM:潤滑をみる
IF PALAM:潤滑 < PALAMLV:1
	TIMES B , 0.60
ELSEIF PALAM:潤滑 < PALAMLV:2
	TIMES B , 0.70
ELSEIF PALAM:潤滑 < PALAMLV:3
	TIMES B , 0.80
ELSEIF PALAM:潤滑 < PALAMLV:4
	TIMES B , 0.90
ELSEIF PALAM:潤滑 >= PALAMLV:4
	TIMES B , 1.00
ENDIF

;調教対象のバストサイズを見る
;爆乳
IF TALENT:爆乳
	TIMES B , 1.50
;巨乳
ELSEIF TALENT:巨乳
	TIMES B , 1.20
;貧乳
ELSEIF TALENT:貧乳
	TIMES B , 0.90
;絶壁
ELSEIF TALENT:絶壁
	TIMES B , 0.70
ENDIF

;プレイヤーのABL:Ｃ感覚をみる
IF ABL:PLAYER:Ｃ感覚 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:Ｃ感覚 == 1
	TIMES B , 1.50
ELSEIF ABL:PLAYER:Ｃ感覚 == 2
	TIMES B , 2.00
ELSEIF ABL:PLAYER:Ｃ感覚 == 3
	TIMES B , 2.50
ELSEIF ABL:PLAYER:Ｃ感覚 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

;再装填処理中は射精ゲージは20分の1に
SIF CFLAG:PLAYER:24 > 0
	B /= 20


SIF TALENT:PLAYER:半人半妖 || TALENT:PLAYER:ふたなり || TALENT:PLAYER:オトコ
	BASE:PLAYER:射精 += B

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 60
LOSEBASE:気力 += 100

SOURCE:情愛 = 400
SOURCE:性行動 = 100
SOURCE:中毒充足 = 400
SOURCE:9 = 150
SOURCE:屈従 = 40
SOURCE:快Ｂ = 150
SOURCE:18 = 300

;上のほうで計算した汚れデータ
SOURCE:反感追加 = Y*10 + 60

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:恭順追加 += 300

;中毒による中毒充足のソースは先に処理しておく
;セックス中毒を見る
IF ABL:セックス中毒 == 1
	TIMES SOURCE:9 , 0.00
ELSEIF ABL:セックス中毒 == 2
	TIMES SOURCE:9 , 0.30
ELSEIF ABL:セックス中毒 == 3
	TIMES SOURCE:9 , 0.60
ELSEIF ABL:セックス中毒 == 4
	TIMES SOURCE:9 , 0.90
ELSEIF ABL:セックス中毒 == 5
	TIMES SOURCE:9 , 1.20
ELSEIF ABL:セックス中毒 == 6
	TIMES SOURCE:9 , 1.50
ELSEIF ABL:セックス中毒 == 7
	TIMES SOURCE:9 , 1.80
ELSEIF ABL:セックス中毒 == 8
	TIMES SOURCE:9 , 2.30
ELSEIF ABL:セックス中毒 == 9
	TIMES SOURCE:9 , 3.00
ELSEIF ABL:セックス中毒 >= 10
	TIMES SOURCE:9 , 5.00
ENDIF

;-------------------------------------------------
;射精チェック
;-------------------------------------------------
CALL SAMEN_CHECK

;射精時の処理
IF E >= 1
	;射精先タイプの設定
	TFLAG:703 = 4
	;射精先選択の可・不可
	TFLAG:704 = 1
	;汎用的な処理はこの関数で行う
	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
	;コマンドごとに固有の処理はこの下に書く
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷の口⇔調教者のＰの汚れが移動
;射精に関係なく行われる処理はここから
STAIN:口 |= STAIN:PLAYER:ペニス
STAIN:PLAYER:ペニス |= STAIN:口
;奴隷のＢ⇔調教者のＰの汚れが移動
STAIN:胸 |= STAIN:PLAYER:ペニス
STAIN:PLAYER:ペニス |= STAIN:胸

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;性交経験
EXP:性交経験 += 1
PRINTS EXPNAME:4
PRINTL +1

;レズ経験
A = 4
B = NO:PLAYER + 899
IF TALENT:オトコ == 0 && TALENT:PLAYER:オトコ == 0
	PRINTFORML %EXPNAME:40%+{A}
	EXP:レズ経験 += A
	CFLAG:B += A
ELSEIF TALENT:オトコ && TALENT:PLAYER:オトコ
	PRINTFORML %EXPNAME:41%+{A}
	EXP:ゲイ経験 += A
	CFLAG:B += A
ENDIF

;愛情経験
IF TALENT:恋慕 == 1 && ASSIPLAY == 0
	PRINTS EXPNAME:23
	PRINTL +1
	EXP:愛情経験 += 2
ELSEIF TALENT:親愛 == 1 && ASSIPLAY == 0
	PRINTS EXPNAME:23
	PRINTL +2
	EXP:愛情経験 += 4
ENDIF

;依存度ベクトル
TFLAG:33 = 1

;依存度補正値(主人かつ恋慕、淫乱、服従で1。親愛、娼婦、隷属で2）
IF (TALENT:恋慕 || TALENT:淫乱 || TALENT:服従) && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF (TALENT:親愛 || TALENT:娼婦 || TALENT:隷属) && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF


RETURN 1

;-------------------------------------------------
;固有の実行判定
;-------------------------------------------------
@COM_ORDER_65
;縄で縛られている場合は無視（縄が無い時のみ実行判定を行う）
IF TEQUIP:44 == 0
	A = 0
	S = 0
	;すべての命令に共通の要素を考慮
	;(従順が高いと命令に従いやすいなど)
	CALL COM_ORDER

	;ABL:欲望
	IF ABL:欲望
		SIF S
			PRINT  + 
		A += ABL:欲望 * 3
		PRINTS ABLNAME:1
		PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 3,')
		S = 1
	ENDIF
	;ABL:Ｂ感覚
	IF ABL:Ｂ感覚
		SIF S
			PRINT  + 
		A += ABL:Ｂ感覚 * 2
		PRINTS ABLNAME:6
		PRINTV 'LV,ABL:Ｂ感覚,'(,ABL:Ｂ感覚 * 2,')
		S = 1
	ENDIF
	;ABL:セックス中毒
	IF ABL:セックス中毒
		SIF S
			PRINT  + 
		A += ABL:セックス中毒 * 1
		PRINTS ABLNAME:18
		PRINTV 'LV,ABL:セックス中毒,'(,ABL:セックス中毒 * 1,')
		S = 1
	ENDIF
	;快楽刻印
	IF MARK:快楽刻印
		SIF S
			PRINT  + 
		A += MARK:快楽刻印 * 3
		PRINTS MARKNAME:1
		PRINTV 'LV,MARK:快楽刻印,'(,MARK:快楽刻印 * 3,')
		S = 1
	ENDIF
	;PALAM:潤滑（の不足）
	IF PALAM:潤滑 < PALAMLV:3
		PRINT  - 
		A -= 5
		PRINT 潤滑不足
		PRINTV '(,5,')
		S = 1
	ENDIF
	;PALAM:欲情
	IF PALAM:欲情 < PALAMLV:1
		L = 0
	ELSEIF PALAM:欲情 < PALAMLV:2
		L = 1
	ELSEIF PALAM:欲情 < PALAMLV:3
		L = 2
	ELSEIF PALAM:欲情 < PALAMLV:4
		L = 3
	ELSEIF PALAM:欲情 < PALAMLV:5
		L = 4
	ELSE
		L = 5
	ENDIF
	IF L
		SIF S
			PRINT  + 
		A += L * 3
		PRINTS PALAMNAME:6
		PRINTV 'LV,L,'(,L * 3,')
		S = 1
	ENDIF
	;恥じらい
	IF TALENT:恥じらい
		PRINT  - 
		A -= 2
		PRINTS TALENTNAME:35
		PRINTV '(,2,')
		S = 1
	ENDIF
	;快感の否定
	IF TALENT:快感の否定
		PRINT  - 
		A -= 5
		PRINTS TALENTNAME:71
		PRINTV '(,5,')
		S = 1
	ENDIF
	;男嫌い（恋慕、親愛で無効）
	IF TALENT:男嫌い && TALENT:PLAYER:オトコ && ((TALENT:恋慕 == 0 && TALENT:親愛 == 0) || PLAYER != MASTER)
		PRINT  - 
		A -= 12
		PRINTS TALENTNAME:82
		PRINTV '(,12,')
		S = 1
	ENDIF
	;恋慕
	IF TALENT:恋慕 && ASSIPLAY == 0
		SIF S
			PRINT  + 
		A += 5
		PRINTS TALENTNAME:3
		PRINTV '(,5,')
		S = 5
	ENDIF
	;親愛
	IF TALENT:親愛 && ASSIPLAY == 0
		SIF S
			PRINT  + 
		A += 10
		PRINTS TALENTNAME:6
		PRINTV '(,10,')
		S = 10
	ENDIF

	;調教者がふたなり
	IF TALENT:PLAYER:ふたなり
		SIF S
			PRINT  + 
		A += 4
		PRINTS TALENTNAME:139
		PRINTV '(,4,')
		S = 1
	ENDIF

	;媚薬
	IF TEQUIP:26
		SIF S
			PRINT  + 
		A += 6
		PRINTS ITEMNAME:41
		PRINTV '(,6,')
		S = 1
	ENDIF

	Y = 0
	;愛液の汚れ
	SIF STAIN:PLAYER:ペニス & 1
		Y += 1
	;精液の汚れ
	SIF STAIN:PLAYER:ペニス & 4
		Y += 3
	;アナルの汚れ
	SIF STAIN:PLAYER:ペニス & 8
		Y += 7
	SIF STAIN:PLAYER:ペニス & 16
		Y += 15

	SIF TALENT:汚臭鈍感
		Y /= 3
	SIF TALENT:汚臭敏感
		Y *= 2

	;汚れあり
	IF Y
		PRINT  - 
		A -= Y
		;汚臭鈍感
		IF TALENT:汚臭鈍感
			PRINT 汚れあり、
			PRINTS TALENTNAME:61
		ELSEIF TALENT:汚臭敏感
			PRINT 汚れあり、
			PRINTS TALENTNAME:62
		ELSE
			PRINT 汚れあり
		ENDIF
		PRINTV '(,Y,')
		S = 1
	ENDIF

	;合計を表示(15以上で実行)
	PRINT  = 
	PRINTV A

	V = 15
	SIF A < V
		PRINT  < 
	SIF A == V
		PRINT  = 
	SIF A > V
		PRINT  > 
	PRINT 実行値
	PRINTV V

	WAIT

	;実行できない
	SIF A < V
		RETURN 0
ENDIF
RETURN 1

