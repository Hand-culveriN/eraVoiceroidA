;-------------------------------------------------
;3P
;セックス系派生コマンド
;-------------------------------------------------
@COM500
#DIM DYNAMIC ORIGINCOM
PRINTL 3P
STR:0 = 3P
ORIGINCOM = SELECTCOM
SELECTCOM = 500
CALL KOJO_MESSAGE_COM


;-------------------------------------------------
;挿入部位管理
;-------------------------------------------------
;直接コマンドを指定した場合
IF ORIGINCOM == 64 || ORIGINCOM == 200
	TFLAG:40 = 1
	TFLAG:41 = 2
ELSE
	SIF ORIGINCOM == 60 || ORIGINCOM == 61 || ORIGINCOM == 66
		A = 1
	SIF ORIGINCOM == 62 || ORIGINCOM == 63 || ORIGINCOM == 67
		A = 2
	SIF ORIGINCOM == 81 || ORIGINCOM == 140
		A = 3
	SIF PREVCOM == 60 || PREVCOM == 61 || PREVCOM == 66
		B = 1
	SIF PREVCOM == 62 || PREVCOM == 63 || PREVCOM == 67
		B = 2
	SIF PREVCOM == 81 || PREVCOM == 140
		B = 3
	;前回コマンドが3Pでない
	IF TFLAG:42 == 0
		;今回調教者が助手=前回調教者が主人
		IF ASSIPLAY
			TFLAG:40 = B
			TFLAG:41 = A
		;今回調教者が主人=前回調教者が助手
		ELSE
			TFLAG:40 = A
			TFLAG:41 = B
		ENDIF
	;前回コマンドが3Pである
	ELSE
		;今回調教者が助手=前回調教者が主人
		IF ASSIPLAY
			;助手の使用部位が前回の主人と同じ場合
			SIF A == TFLAG:40
				TFLAG:40 = TFLAG:41
			TFLAG:41 = A
		ELSE
			;主人の使用部位が前回の助手と同じ場合
			SIF A == TFLAG:41
				TFLAG:41 = TFLAG:40
			TFLAG:40 = A
		ENDIF
	ENDIF
ENDIF

;V経験を伴うコマンドのフラグ(処女膜再生対応)
SIF TFLAG:40 == 1 || TFLAG:41 == 1
	TFLAG:19 = 1

SIF (TALENT:恋慕 || TALENT:親愛) && ASSIPLAY == 0 && EXP:Ｖ経験 == 0 && TFLAG:40 == 1
	TFLAG:20 = 1

;-------------------------------------------------
;射精ゲージチェック（主人）
;-------------------------------------------------
B = 2500

;ABL:調教者の技巧をみる
IF ABL:PLAYER:技巧 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:技巧 == 1
	TIMES B , 1.20
ELSEIF ABL:PLAYER:技巧 == 2
	TIMES B , 1.40
ELSEIF ABL:PLAYER:技巧 == 3
	TIMES B , 1.60
ELSEIF ABL:PLAYER:技巧 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.00
ENDIF

;ABL:従順をみる
IF ABL:従順 == 0
	TIMES B , 0.80
ELSEIF ABL:従順 == 1
	TIMES B , 0.90
ELSEIF ABL:従順 == 2
	TIMES B , 1.00
ELSEIF ABL:従順 == 3
	TIMES B , 1.10
ELSEIF ABL:従順 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:欲望をみる
IF ABL:欲望 == 0
	TIMES B , 1.00
ELSEIF ABL:欲望 == 1
	TIMES B , 1.10
ELSEIF ABL:欲望 == 2
	TIMES B , 1.20
ELSEIF ABL:欲望 == 3
	TIMES B , 1.30
ELSEIF ABL:欲望 == 4
	TIMES B , 1.40
ELSE
	TIMES B , 1.50
ENDIF

;VとAに挿入時、PALAM:潤滑をみる
IF TFLAG:40 <= 2
	IF PALAM:潤滑 < PALAMLV:1
		TIMES B , 0.60
	ELSEIF PALAM:潤滑 < PALAMLV:2
		TIMES B , 0.80
	ELSEIF PALAM:潤滑 < PALAMLV:3
		TIMES B , 1.00
	ELSEIF PALAM:潤滑 < PALAMLV:4
		TIMES B , 1.20
	ELSEIF PALAM:潤滑 >= PALAMLV:4
		TIMES B , 1.40
	ENDIF
ENDIF

;プレイヤーのABL:Ｃ感覚をみる
IF ABL:PLAYER:Ｃ感覚 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:Ｃ感覚 == 1
	TIMES B , 1.50
ELSEIF ABL:PLAYER:Ｃ感覚 == 2
	TIMES B , 2.00
ELSEIF ABL:PLAYER:Ｃ感覚 == 3
	TIMES B , 2.50
ELSEIF ABL:PLAYER:Ｃ感覚 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

;EXP:Ｖ経験をみる
;処女だと増える
SIF EXP:Ｖ経験 < EXPLV:1
	TIMES B , 1.50

SIF TALENT:MASTER:半人半妖 || TALENT:MASTER:ふたなり || TALENT:MASTER:オトコ
	BASE:MASTER:射精 += B

;-------------------------------------------------
;射精ゲージチェック（助手）
;-------------------------------------------------
B = 2500

;ABL:調教者の技巧をみる
IF ABL:PLAYER:技巧 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:技巧 == 1
	TIMES B , 1.20
ELSEIF ABL:PLAYER:技巧 == 2
	TIMES B , 1.40
ELSEIF ABL:PLAYER:技巧 == 3
	TIMES B , 1.60
ELSEIF ABL:PLAYER:技巧 == 4
	TIMES B , 1.80
ELSE
	TIMES B , 2.00
ENDIF

;ABL:従順をみる
IF ABL:従順 == 0
	TIMES B , 0.80
ELSEIF ABL:従順 == 1
	TIMES B , 0.90
ELSEIF ABL:従順 == 2
	TIMES B , 1.00
ELSEIF ABL:従順 == 3
	TIMES B , 1.10
ELSEIF ABL:従順 == 4
	TIMES B , 1.20
ELSE
	TIMES B , 1.30
ENDIF

;ABL:欲望をみる
IF ABL:欲望 == 0
	TIMES B , 1.00
ELSEIF ABL:欲望 == 1
	TIMES B , 1.10
ELSEIF ABL:欲望 == 2
	TIMES B , 1.20
ELSEIF ABL:欲望 == 3
	TIMES B , 1.30
ELSEIF ABL:欲望 == 4
	TIMES B , 1.40
ELSE
	TIMES B , 1.50
ENDIF

;VとAに挿入時、PALAM:潤滑をみる
IF TFLAG:41 <= 2
	IF PALAM:潤滑 < PALAMLV:1
		TIMES B , 0.60
	ELSEIF PALAM:潤滑 < PALAMLV:2
		TIMES B , 0.80
	ELSEIF PALAM:潤滑 < PALAMLV:3
		TIMES B , 1.00
	ELSEIF PALAM:潤滑 < PALAMLV:4
		TIMES B , 1.20
	ELSEIF PALAM:潤滑 >= PALAMLV:4
		TIMES B , 1.40
	ENDIF
ENDIF

;プレイヤーのABL:Ｃ感覚をみる
IF ABL:PLAYER:Ｃ感覚 == 0
	TIMES B , 1.00
ELSEIF ABL:PLAYER:Ｃ感覚 == 1
	TIMES B , 1.50
ELSEIF ABL:PLAYER:Ｃ感覚 == 2
	TIMES B , 2.00
ELSEIF ABL:PLAYER:Ｃ感覚 == 3
	TIMES B , 2.50
ELSEIF ABL:PLAYER:Ｃ感覚 == 4
	TIMES B , 3.50
ELSE
	TIMES B , 5.00
ENDIF

SIF TALENT:MASTER:半人半妖 || TALENT:MASTER:ふたなり || TALENT:MASTER:オトコ
	BASE:ASSI:射精 += B


;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:体力 += 160
LOSEBASE:気力 += 350

SOURCE:性行動 = 300
SOURCE:達成感 = 100
SOURCE:9 = 500
SOURCE:逸脱 = 500
SOURCE:快Ｂ = 500
SOURCE:18 = 500

;挿入している部位によって、ソースの数値が変化する
;Vに挿入している場合
IF TFLAG:40 == 1 || TFLAG:41 == 1
	SOURCE:快Ｖ += 400
	SOURCE:達成感 += 100
	SOURCE:中毒充足 += 500
ENDIF
;Aに挿入している場合
IF TFLAG:40 == 2 || TFLAG:41 == 2
	SOURCE:快Ａ += 400
	SOURCE:達成感 += 100
	SOURCE:中毒充足 += 800
ENDIF
;口に挿入している場合
IF TFLAG:40 == 3 || TFLAG:41 == 3
	SOURCE:達成感 += 420
	SOURCE:痛み += 150
ENDIF


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:抑圧 || TALENT:抵抗
	SOURCE:恭順追加 += 500

;貞操観念持ちで恋慕、親愛持ち以外には常に反発のソースを追加する（セックス系のみ）
SIF TALENT:貞操観念 && (TALENT:恋慕 == 0 && TALENT:親愛 == 0)
	SOURCE:18 += 1000

;Vに挿入している場合
IF TFLAG:40 == 1 || TFLAG:41 == 1
	;処女だった場合は、苦痛のソースと反発のソースを追加する
	IF TALENT:処女 == 1
		SOURCE:中毒充足 += 5500
		SOURCE:18 += 3000
	ENDIF

	;再生処女だった場合は、苦痛のソースと反発のソースを追加する
	IF TALENT:再生処女 == 1
		SOURCE:中毒充足 += 5500
		SOURCE:18 += 1000
	ENDIF

	;Vへの苦痛は先に処理しておく
	;EXP:Ｖ経験をみる（処女の反感は別途処理済みなので省略）
	IF EXP:Ｖ経験 < EXPLV:1
		TIMES SOURCE:中毒充足 , 2.50
	ELSEIF EXP:Ｖ経験 < EXPLV:2
		TIMES SOURCE:中毒充足 , 0.80
		TIMES SOURCE:18 , 1.20
	ELSEIF EXP:Ｖ経験 < EXPLV:3
		TIMES SOURCE:中毒充足 , 0.40
	ELSEIF EXP:Ｖ経験 < EXPLV:4
		TIMES SOURCE:中毒充足 , 0.10
	ELSEIF EXP:Ｖ経験 < EXPLV:5
		TIMES SOURCE:中毒充足 , 0.00
	ELSEIF EXP:Ｖ経験 >= EXPLV:5
		TIMES SOURCE:中毒充足 , 0.00
	ENDIF

	;PALAM:潤滑をみる
	IF PALAM:潤滑 < PALAMLV:1
		TIMES SOURCE:中毒充足 , 1.20
		TIMES SOURCE:18 , 1.50
	ELSEIF PALAM:潤滑 < PALAMLV:2
		TIMES SOURCE:中毒充足 , 0.70
		TIMES SOURCE:18 , 1.20
	ELSEIF PALAM:潤滑 < PALAMLV:3
		TIMES SOURCE:中毒充足 , 0.40
	ELSEIF PALAM:潤滑 < PALAMLV:4
		TIMES SOURCE:中毒充足 , 0.10
	ELSEIF PALAM:潤滑 >= PALAMLV:4
		TIMES SOURCE:中毒充足 , 0.10
	ENDIF
ENDIF

;Aに挿入している場合
IF TFLAG:40 == 2 || TFLAG:41 == 2
	;Aへの苦痛は先に処理しておく
	;EXP:Ａ経験をみる
	IF EXP:Ａ経験 < EXPLV:1
		TIMES SOURCE:中毒充足 , 3.00
		TIMES SOURCE:18 , 3.00
	ELSEIF EXP:Ａ経験 < EXPLV:2
		TIMES SOURCE:中毒充足 , 1.20
		TIMES SOURCE:18 , 2.00
	ELSEIF EXP:Ａ経験 < EXPLV:3
		TIMES SOURCE:中毒充足 , 0.80
		TIMES SOURCE:18 , 1.50
	ELSEIF EXP:Ａ経験 < EXPLV:4
		TIMES SOURCE:中毒充足 , 0.60
		TIMES SOURCE:18 , 1.20
	ELSEIF EXP:Ａ経験 < EXPLV:5
		TIMES SOURCE:中毒充足 , 0.40
	ELSEIF EXP:Ａ経験 >= EXPLV:5
		TIMES SOURCE:中毒充足 , 0.20
	ENDIF

	;PALAM:潤滑をみる
	IF PALAM:潤滑 < PALAMLV:1
		TIMES SOURCE:中毒充足 , 1.20
		TIMES SOURCE:18 , 2.00
	ELSEIF PALAM:潤滑 < PALAMLV:2
		TIMES SOURCE:中毒充足 , 1.00
		TIMES SOURCE:18 , 1.50
	ELSEIF PALAM:潤滑 < PALAMLV:3
		TIMES SOURCE:中毒充足 , 0.60
	ELSEIF PALAM:潤滑 < PALAMLV:4
		TIMES SOURCE:中毒充足 , 0.30
	ELSEIF PALAM:潤滑 >= PALAMLV:4
		TIMES SOURCE:中毒充足 , 0.20
	ENDIF
ENDIF

;セックス中毒を見る
IF ABL:セックス中毒 == 1
	TIMES SOURCE:9 , 0.00
ELSEIF ABL:セックス中毒 == 2
	TIMES SOURCE:9 , 0.30
ELSEIF ABL:セックス中毒 == 3
	TIMES SOURCE:9 , 0.60
ELSEIF ABL:セックス中毒 == 4
	TIMES SOURCE:9 , 0.90
ELSEIF ABL:セックス中毒 == 5
	TIMES SOURCE:9 , 1.20
ELSEIF ABL:セックス中毒 == 6
	TIMES SOURCE:9 , 1.50
ELSEIF ABL:セックス中毒 == 7
	TIMES SOURCE:9 , 1.80
ELSEIF ABL:セックス中毒 == 8
	TIMES SOURCE:9 , 2.30
ELSEIF ABL:セックス中毒 == 9
	TIMES SOURCE:9 , 3.00
ELSEIF ABL:セックス中毒 >= 10
	TIMES SOURCE:9 , 5.00
ENDIF


;-------------------------------------------------
;射精チェック１（主人）
;-------------------------------------------------
CALL SAMEN_CHECK_M

;射精時の処理
IF E >= 1
	;射精先タイプの設定
	IF TFLAG:40 == 1
		TFLAG:703 = 1
	ELSEIF TFLAG:40 == 2
		TFLAG:703 = 2
	ELSE
		TFLAG:703 = 4
	ENDIF
	;射精先選択の可・不可
	TFLAG:704 = 0
	;汎用的な処理はこの関数で行う
	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

;-------------------------------------------------
;射精チェック２（助手）
;-------------------------------------------------
CALL SAMEN_CHECK_A

;射精時の処理
IF E >= 1
	;射精先タイプの設定
	TFLAG:703 = 12
	;射精先選択の可・不可
	TFLAG:704 = 0
	;汎用的な処理はこの関数で行う
	;（射精ゲージの再計算や射精経験の上昇と共通部分の表示）
	CALL SAMEN_SHOOT
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
IF TFLAG:40 == 1
	;膣内に破瓜の血の汚れを付加
	SIF TALENT:処女 == 1 || TALENT:再生処女 == 1
		STAIN:6 |= 64
	;奴隷の膣内⇔主人のＰの汚れが移動
	IF TALENT:MASTER:半人半妖 || TALENT:MASTER:ふたなり || TALENT:MASTER:オトコ
		STAIN:6 |= STAIN:MASTER:ペニス
		STAIN:MASTER:ペニス |= STAIN:6
	ENDIF
ELSEIF TFLAG:40 == 2
	;奴隷のＡ⇔主人のＰの汚れが移動
	IF TALENT:MASTER:半人半妖 || TALENT:MASTER:ふたなり || TALENT:MASTER:オトコ
		STAIN:アナル |= STAIN:MASTER:ペニス
		STAIN:MASTER:ペニス |= STAIN:アナル
	ENDIF
ELSEIF TFLAG:40 == 3
	;奴隷の口⇔主人のＰの汚れが移動
	IF TALENT:MASTER:半人半妖 || TALENT:MASTER:ふたなり || TALENT:MASTER:オトコ
		STAIN:口 |= STAIN:MASTER:ペニス
		STAIN:MASTER:ペニス |= STAIN:口
	ENDIF
ENDIF

IF TFLAG:41 == 1
	;膣内に破瓜の血の汚れを付加
	SIF TALENT:処女 == 1 || TALENT:再生処女 == 1
		STAIN:6 |= 64
	;奴隷の膣内⇔助手のＰの汚れが移動
	IF TALENT:ASSI:ふたなり || TALENT:ASSI:オトコ
		STAIN:6 |= STAIN:ASSI:ペニス
		STAIN:ASSI:ペニス |= STAIN:6
	ENDIF
ELSEIF TFLAG:41 == 2
	;奴隷のＡ⇔助手のＰの汚れが移動
	IF TALENT:ASSI:ふたなり || TALENT:ASSI:オトコ
		STAIN:アナル |= STAIN:ASSI:ペニス
		STAIN:ASSI:ペニス |= STAIN:アナル
	ENDIF
ELSEIF TFLAG:41 == 3
	;奴隷の口⇔助手のＰの汚れが移動
	IF TALENT:ASSI:ふたなり || TALENT:ASSI:オトコ
		STAIN:口 |= STAIN:ASSI:ペニス
		STAIN:ASSI:ペニス |= STAIN:口
	ENDIF
ENDIF


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;Vに挿入している場合
IF TFLAG:40 == 1 || TFLAG:41 == 1
	EXP:Ｖ経験 += 1
	PRINTL Ｖ経験＋１
ENDIF

;Aに挿入している場合
IF TFLAG:40 == 2 || TFLAG:41 == 2
	EXP:Ａ経験 += 2
	PRINTL Ａ経験＋２
ENDIF

;性交経験
;VとAに挿入している場合（口に挿入していない場合）
IF TFLAG:40 != 3 && TFLAG:41 != 3
	EXP:性交経験 += 2
	PRINTS EXPNAME:4
	PRINTL +2
ELSE
	EXP:性交経験 += 1
	PRINTS EXPNAME:4
	PRINTL +1
ENDIF

;異常経験
IF (TFLAG:40 == 1 || TFLAG:41 == 1) && TALENT:処女 == 1
	EXP:異常経験 += 2
	PRINTFORML %EXPNAME:50%＋２
ENDIF

;レズ経験
IF TALENT:オトコ == 0 && TALENT:MASTER:オトコ == 0 && TALENT:ASSI:オトコ == 0
	PRINTS EXPNAME:40
	PRINTL +10
	EXP:レズ経験 += 10
	A = NO:MASTER + 899
	CFLAG:A += 5
	A = NO:ASSI + 899
	CFLAG:A += 5
;レズ経験
ELSEIF TALENT:オトコ == 0 && TALENT:MASTER:オトコ == 0 && TALENT:ASSI:オトコ == 1
	PRINTS EXPNAME:40
	PRINTL +5
	EXP:レズ経験 += 5
	A = NO:MASTER + 899
	CFLAG:A += 5
;レズ経験
ELSEIF TALENT:オトコ == 0 && TALENT:MASTER:オトコ == 1 && TALENT:ASSI:オトコ == 0
	PRINTS EXPNAME:40
	PRINTL +5
	EXP:レズ経験 += 5
	A = NO:ASSI + 899
	CFLAG:A += 5
;ゲイ経験
ELSEIF TALENT:オトコ == 1 && TALENT:MASTER:オトコ == 0 && TALENT:ASSI:オトコ == 1
	PRINTS EXPNAME:40
	PRINTL +5
	EXP:ゲイ経験 += 5
	A = NO:MASTER + 899
	CFLAG:A += 5
;ゲイ経験
ELSEIF TALENT:オトコ == 1 && TALENT:MASTER:オトコ == 1 && TALENT:ASSI:オトコ == 0
	PRINTS EXPNAME:41
	PRINTL +5
	EXP:ゲイ経験 += 5
	A = NO:ASSI + 899
	CFLAG:A += 5
;ゲイ経験
ELSEIF TALENT:オトコ == 1 && TALENT:MASTER:オトコ == 1 && TALENT:ASSI:オトコ == 1
	PRINTS EXPNAME:41
	PRINTL +5
	EXP:ゲイ経験 += 5
	A = NO:ASSI + 899
	CFLAG:A += 5
ENDIF

;依存度ベクトル
TFLAG:33 = 1

;依存度補正値(主人かつ恋慕で1、親愛で2）
IF TALENT:恋慕 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:親愛 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF


RETURN 1

