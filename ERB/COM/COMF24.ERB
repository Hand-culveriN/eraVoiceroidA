;-------------------------------------------------
;スキンシップ
;コミュニケーション系コマンド、レベル2
;-------------------------------------------------
@COM24

PRINTL スキンシップ
STR:0 = スキンシップ
CALL KOJO_MESSAGE_COM

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
LOSEBASE:0 += 25
LOSEBASE:1 += 25

SOURCE:4 = 500
SOURCE:5 = 500
SOURCE:17 = 100


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;愛情経験
IF (TALENT:3 == 1 || TALENT:6 == 1) && ASSIPLAY == 0
	PRINTS EXPNAME:23
	PRINTL +1
	EXP:23 += 1
ENDIF

;依存度ベクトル
TFLAG:33 = 4

;依存度補正値(恋慕で1、親愛で2）
IF TALENT:3 && ASSIPLAY == 0
	TFLAG:30 += 1
ELSEIF TALENT:6 && ASSIPLAY == 0
	TFLAG:30 += 2
ENDIF


RETURN 1

